                    <li data-id="1" class="active mr-1">
                        <a href="#">Hotels</a>
                    </li>
                    <li data-id="2" class="mr-1">
                        <a href="#">Flights</a>
                    </li>
                    <li data-id="3" class="mr-1">
                        <a href="#">Flight & Hotels</a>
                    </li>
                    <li data-id="4" class="mr-1">
                        <a href="#">Cars</a>
                    </li>
                    <li data-id="5" class="mr-1">
